generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Wishlist {
  id        String   @id @default(uuid())
  email     String   @unique
  phone     String?
  role      String
  createdAt DateTime @default(now())
}
